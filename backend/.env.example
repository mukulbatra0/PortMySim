# MongoDB Connection String
MONGODB_URI=mongodb://localhost:27017/portmysim

# Server Port
PORT=5000

# Node Environment
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRE=30d

# Email Configuration
EMAIL_HOST=smtp.ethereal.email
EMAIL_PORT=587
EMAIL_SECURE=false
EMAIL_USER=your_email_username
EMAIL_PASSWORD=your_email_password
EMAIL_FROM=support@portmysim.com

# Twilio SMS Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Fast2SMS Configuration (for Indian numbers)
FAST2SMS_API_KEY=your_fast2sms_api_key
FAST2SMS_ROUTE=q

# Fast2SMS Template IDs
FAST2SMS_TEMPLATE_PORT_CONF=your_template_id
FAST2SMS_TEMPLATE_PORT_SMS=your_template_id
FAST2SMS_TEMPLATE_PORT_CENTER=your_template_id
FAST2SMS_TEMPLATE_PORT_DOCS=your_template_id
FAST2SMS_TEMPLATE_PORT_ACTIVE=your_template_id
FAST2SMS_TEMPLATE_PORT_UPC=your_template_id

# Notification Scheduler
NOTIFICATION_INTERVAL_MINUTES=5

# Location API Keys
LOCATION_IQ_API_KEY=your_locationiq_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Telecom Provider API Keys - Airtel
AIRTEL_API_KEY=your_airtel_api_key
